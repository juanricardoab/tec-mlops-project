    "def one_hot_encoding(df_original, columna_encoding):\n",
    "    dummies = pd.get_dummies(df_original[[columna_encoding]])\n",
    "    res_df = pd.concat([df_original, dummies], axis=1)\n",
    "    res_df.drop([columna_encoding], axis='columns', inplace = True)\n",
    "    return(res_df)"
